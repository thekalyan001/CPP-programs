#include<bits/stdc++.h>
using namespace std;
int fun(int v,int a[100])
{
	int m,p;
  //taking the value inside the v number of the array v is nothing but the number taken inside the main
  cout<<"enter the input\n";
	for(int i=0;i<v;i++)
	{
		cin>>a[i];
	}
	cout<<"enter the nuber for find\n";
	cin>>m;
	for(int i=0;i<v;i++)
{
	if(a[i]==m)       //if this is satisfy then it will return the location of the number
	{
	return i;
	}
	

}
return -1;

}
int main()
{
	int n,a[100],m;
	cin>>n;         //taking the numer fo the value inside the array
	cout<<"the number is present at"<<fun(n,a)<<endl;       //calling the function for value
}
