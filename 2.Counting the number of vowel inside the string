#include<iostream>
#include<string.h>
using namespace std;
int main()
{
	int i,n,v=0;
	char *a;
gets(a);
	n=strlen(a);
	for(i=0;i<n;i++)
	{
	if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')  //if this satisfy then v++ increment
		{
     v++;
 }
}
		cout<<"The number of the voowel occouring is "<<v;
		return 0;


}
