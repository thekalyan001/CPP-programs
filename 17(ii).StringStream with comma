//Given an input string which is comma-separated instead of space, the task is to parse this input string in C++.
#include <bits/stdc++.h> 
using namespace std; 
  
int main() 
{ 
    string str; 
  
    // Get the string 
    getline(cin, str); 
  
    // Print the words 
    istringstream ss(str); 
    do { 
        string word; 
        ss >> word; 
        cout << word << endl; 
    } while (ss); 
} 

/*
Input:
1 2 3 4 5 6
Output:

1
2
3
4
5
6
*/
